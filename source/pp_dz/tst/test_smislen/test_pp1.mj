class P
	final int size = 10;
	int pos[];
{
	void main()
		int x, i;

	{ //---------- Initialize val

		pos = new int[size];
		i = 0;
		while (i < size) {
			pos[i] = 0;
			i++;
		}
		//---------- Read values
		read(x);
		while (x > 0) {
			if (x < size) {
				pos[x]++;
			}
			read(x);
		}
		
		i = 0;
		while(i < size){
			print(pos[i]);
			i++;
		}
	}
}